{
  "presets": ["next/babel"],
  "plugins": [
    [
      "import",
      {
        "libraryName": "antd",
        "style": "less"
      }
    ],
    [
      "module-resolver",
      {
        "root": ["./"],
        "alias": {
          "components": "./components",
          "lib": "./lib"
        }
      }
    ],
    [
      "lodash",
      {
        "id": ["lodash", "recompose"]
      }
    ]
  ]
}